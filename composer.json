{
  "name": "slavcodev/php-json-pointer",
  "description": "An easy to use, implementation of the RFC 6901",
  "license": "MIT",
  "homepage": "https://github.com/slavcodev/php-json-pointer",
  "type": "library",
  "authors": [
    {
      "name": "Veaceslav Medvedev",
      "email": "slavcopost@gmail.com",
      "homepage": "https://github.com/slavcodev"
    }
  ],
  "config": {
    "sort-packages": true,
    "discard-changes": true
  },
  "minimum-stability": "stable",
  "require": {
    "php": "^7.4|^8.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "vimeo/psalm": "^4.6",
    "friendsofphp/php-cs-fixer": "^2.18",
    "psalm/plugin-phpunit": "^0.15.1"
  },
  "autoload": {
    "psr-4": {
      "Slavcodev\\JsonPointer\\": [
        "./src"
      ]
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Slavcodev\\JsonPointer\\Tests\\": [
        "./tests"
      ]
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  }
}
